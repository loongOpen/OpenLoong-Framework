{
    "workbench.colorTheme": "Default Dark+",
    "window.dialogStyle": "custom",
    "window.titleBarStyle": "custom",
    "files.autoSave": "onFocusChange",
    "editor.renderWhitespace": "all",
    "C_Cpp.vcFormat.newLine.beforeElse": false,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
    "C_Cpp.vcFormat.space.beforeBlockOpenBrace": false,
    "C_Cpp.vcFormat.space.pointerReferenceAlignment": "ignore",
    "C_Cpp.vcFormat.space.withinInitializerListBraces": false,
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.vcFormat.newLine.beforeCatch": false,
    "C_Cpp.default.intelliSenseMode": "linux-gcc-arm64",
    "C_Cpp.vcFormat.space.aroundAssignmentOperator": "ignore",
    "C_Cpp.vcFormat.space.aroundBinaryOperator": "ignore",
    "window.openWithoutArgumentsInNewWindow": "off",
    "window.restoreWindows": "none",
    "cmake.showOptionsMovedNotification": false,
    "editor.minimap.enabled": false,
    "editor.fontSize": 16,
    "search.seedOnFocus": true,
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "search.seedWithNearestWord": true,
    "explorer.confirmDelete": false,
    "window.zoomLevel": 1,
    "terminal.integrated.allowChords": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "security.workspace.trust.untrustedFiles": "open",
    "explorer.confirmPasteNative": false,
    "git.terminalAuthentication": false,
    "git.ignoreMissingGitWarning": true,
    "cmake.configureOnOpen": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "git.autofetch": true,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "explorer.confirmDragAndDrop": false,
    "git.openRepositoryInParentFolders": "never",
    "update.mode": "none",
    "vscode-pydata-viewer.pythonPath": "/home/yyp/miniconda3",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "editor.insertSpaces": false,
    "workbench.editorAssociations": {
        "{git,gitlens}:/**/*.{md,csv,svg}": "default"
    },
    "C_Cpp.vcFormat.space.aroundTernaryOperator": "ignore",
    "C_Cpp.vcFormat.space.beforeKeywordsInControlFlowStatements": false,
    "C_Cpp.vcFormat.space.afterKeywordsInControlFlowStatements": false,
    "C_Cpp.vcFormat.indent.preprocessor": "none",
    "C_Cpp.vcFormat.indent.preserveComments": true,
    "C_Cpp.vcFormat.indent.preserveWithinParentheses": true,
    "C_Cpp.vcFormat.wrap.preserveBlocks": "allOneLineScopes",
    "C_Cpp.formatting": "vcFormat",
    "vscode-office.openOutline": true,
}